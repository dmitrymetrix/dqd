<div class="page-header">
  <h1>Новы водгук</h1>
</div>

<%= form_for(@testimonial) do |f| %>

    <%= f.label :'Імя/Назва' %>
    <%= f.text_field :user_name %>

    <%= f.label :Водгук %>
    <%= f.text_field :feedback %>

    <%= f.submit 'Дадаць водгук', class: 'btn btn-large btn-primary' %>
<% end %>
